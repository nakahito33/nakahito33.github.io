<span class="hljs-comment"># Read the CSV file into a DataFrame</span>
df = pd.read_csv(<span class="hljs-string">"ALICE_IN_WONDERLAND.csv"</span>)

<span class="hljs-comment"># Function to convert time string (HH:MM:SS,mmm) to seconds</span>
<span class="hljs-function"><span class="hljs-keyword">def</span> <span class="hljs-title">time_to_seconds</span>(<span class="hljs-params">time_str</span>):</span>
    <span class="hljs-keyword">try</span>:
        <span class="hljs-keyword">if</span> <span class="hljs-built_in">isinstance</span>(time_str, <span class="hljs-built_in">str</span>):
            parts = time_str.replace(<span class="hljs-string">','</span>, <span class="hljs-string">'.'</span>).split(<span class="hljs-string">':'</span>)
            <span class="hljs-keyword">if</span> <span class="hljs-built_in">len</span>(parts) == <span class="hljs-number">3</span>:
                h, m, s = <span class="hljs-built_in">float</span>(parts[<span class="hljs-number">0</span>]), <span class="hljs-built_in">float</span>(parts[<span class="hljs-number">1</span>]), <span class="hljs-built_in">float</span>(parts[<span class="hljs-number">2</span>])
                <span class="hljs-keyword">return</span> h * <span class="hljs-number">3600</span> + m * <span class="hljs-number">60</span> + s
            <span class="hljs-keyword">elif</span> <span class="hljs-built_in">len</span>(parts) == <span class="hljs-number">2</span>:
                m, s = <span class="hljs-built_in">float</span>(parts[<span class="hljs-number">0</span>]), <span class="hljs-built_in">float</span>(parts[<span class="hljs-number">1</span>].replace(<span class="hljs-string">','</span>, <span class="hljs-string">'.'</span>))
                <span class="hljs-keyword">return</span> m * <span class="hljs-number">60</span> + s
    <span class="hljs-keyword">except</span> (ValueError, IndexError):
        <span class="hljs-keyword">return</span> <span class="hljs-literal">None</span>
    <span class="hljs-keyword">return</span> <span class="hljs-literal">None</span>

